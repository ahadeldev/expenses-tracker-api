{
  "name": "expenses_tracker",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "db:init": "sequelize-cli init",
    "sb:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate --config config/migrationConfig.js",
    "db:migrate:undo": "sequelize-cli db:migrate:undo --config config/migrationConfig.js",
    "db:generate": "sequelize-cli migration:generate --name",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all"
  },
